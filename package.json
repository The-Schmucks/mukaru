		"@googlemaps/js-api-loader": "^1.16.2",
		"@sentry/node": "^7.73.0",
		"@sentry/svelte": "^7.73.0",
		"axios": "^1.5.1",
		"svelte-icons-pack": "^2.1.0",
		"tailwind-merge": "^1.14.0"
	},
	"packageManager": "pnpm@8.9.0",
	"engines": {
		"pnpm": "^8.0.0"
	},
	"pnpm": {
		"overrides": {
			"@sveltejs/kit": "../tik/packages/kit",
			"@sveltejs/adapter-auto": "../tik/packages/adapter-auto"
		}
